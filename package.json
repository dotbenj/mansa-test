{
  "name": "mansa-test",
  "version": "1.0.0",
  "description": "technical test for mansa",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beninja/mansa-test.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Benjamin ROULLET",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beninja/mansa-test.git/issues"
  },
  "homepage": "https://github.com/beninja/mansa-test.git#readme",
  "devDependencies": {
    "@types/node": "^12.7.8",
    "dotenv": "^8.1.0",
    "husky": "^2.4.1",
    "nodemon": "^1.19.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
